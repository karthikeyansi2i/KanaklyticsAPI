{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet build Kanaklytics.API.csproj",
      "type": "shell",
      "group": "build",
      "problemMatcher": ["$msCompile"]
    },
    {
      "label": "run",
      "command": "dotnet run --project Kanaklytics.API.csproj",
      "type": "shell",
      "group": "test",
      "problemMatcher": []
    },
    {
      "label": "restore",
      "command": "dotnet restore Kanaklytics.API.csproj",
      "type": "shell",
      "group": "none",
      "problemMatcher": []
    }
  ]
}
